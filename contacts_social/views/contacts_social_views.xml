<odoo>
  <data>
  	<record id="view_partner_social_form" model="ir.ui.view">
		<field name="name">res.partner.res_partner_social.form.inherit</field>
		<field name="model">res.partner</field>
		<field name="priority">2</field>
		<field name="inherit_id" ref="base.view_partner_form"/>
		<field name="arch" type="xml">
		<xpath expr="//notebook" position="inside">
			<page string="Social">
				<group>
					<group>
						<field name="facebook"/>
						<field name="facebook_page" widget="url" />
						<field name="instagram"/>
						<field name="google_plus"/>
						<field name="github" widget="url" placeholder="e.g. www.odoo.com"/>
					</group>
					<group>
						<field name="twitter"/>
						<field name="linkedin"/>
						<field name="youtube" widget="url" placeholder="e.g. www.odoo.com"/>
						<field name="blog1" widget="url" placeholder="e.g. www.odoo.com"/>
						<field name="blog2" widget="url" placeholder="e.g. www.odoo.com"/>
					</group>
				</group>
            	
           </page>
		</xpath>
		</field>
	</record>

  </data>
</odoo>