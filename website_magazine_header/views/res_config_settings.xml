<odoo>
  <data>
  	<record id="res_config_settings_view_form" model="ir.ui.view">
    	<field name="name">res.config.settings.view.form.inherit.search</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="20"/>
        <field name="inherit_id" ref="website.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="webmaster_settings" position="after">
            	<h2>Search</h2>
            	<div class="row mt16 o_settings_container" id="webmaster_settings">
                	<div class="col-xs-12 col-md-6 o_setting_box" id="domain_setting">
                		<div class="o_setting_right_pane">
                			<div class="content-group">
                            	<div class="row mt16">
                                	<label class="col-md-3 o_light_label" string="Blog to Search"/>
                                    <field name="search_blog_id"/>
                                </div>
                            </div>
                		</div>
                	</div>
                </div>
            </div>
        </field>
	</record>            
 
  </data>
</odoo>