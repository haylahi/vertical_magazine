<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

		<template id="assets_frontend" inherit_id="web.assets_frontend" name="UG Assets">
		    <xpath expr="//link[last()]" position="after">
		        <link rel="stylesheet" href="/website_unitegallery/static/src/unitegallery/css/unite-gallery.css"/>
		    </xpath>
		    <xpath expr="//script[last()]" position="after">
		        <script type="text/javascript" src="/website_unitegallery/static/src/unitegallery/js/unitegallery.min.js"/>
		        <script type="text/javascript" src="/website_unitegallery/static/src/js/website_unitegallery.js"/>
		    </xpath>
		</template>
		
		<template id="assets_editor" inherit_id="website.assets_editor" name="UG Assets Editor">
			 <xpath expr="//script[last()]" position="after">			 	
		        <script type="text/javascript" src="/website_unitegallery/static/src/js/website_unitegallery.editor.js"/>
			 </xpath>
		</template>
	</data>
</odoo>