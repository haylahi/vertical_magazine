<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.blog</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="20"/>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='domain_setting']" position="after">
                	<div class="col-xs-12 col-md-6 o_setting_box" id="special_settings">
                		<div class="o_setting_right_pane">
                			<label string="Special Settings"/>
                            <div class="text-muted">
                               Unite Gallery
                            </div>
                             <div class="content-group">
                             	<div class="row mt16">
                                  <label class="col-md-3 o_light_label" string="Unite Gallery"/>
                                  <field name="unitegallery_id"/>
                                </div>
                               
                             </div>                		
                		</div>
					</div>                
                </xpath>
            </field>
           </record>
    
    </data>
</odoo>