    	 <record id="ug_default" model="ir.ui.view">
            <field name="name">Default Gallery</field>
            <field name="type">qweb</field>
            <field name="key">website.ug_default</field>
            <field name="arch" type="xml">
                <t name="Default Gallery" t-name="website.ug_default">
                    <t t-call="website.layout">
                        <div id="wrap">
                            <div class="oe_structure">
                            </div>
                         </div>
                    </t>
                </t>
           </field>
        </record>
        
       <record id="ug_default_page" model="website.page">
            <field name="website_published">True</field>
            <field name="url">/ug_default</field>
            <field name="view_id" ref="ug_default" />
        </record>
        
