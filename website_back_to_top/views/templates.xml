<odoo>
    <data>
    
    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Brand Nav-bar Front-end assets">
	    	<xpath expr="." position="inside">
	      		<link type="text/css" rel="stylesheet" href="/website_back_to_top/static/src/css/top.css"/>
	    	</xpath>
		</template>
    
    
    	 <template id="back_to_top" name="Brand Navigation Bar Scripts"  inherit_id="website.layout">
        	 <xpath expr="//div[@id='wrapwrap']" position="after">
        	 	<a href="#0" class="cd-top js-cd-top">Top</a>
        	 	<script src="/website_back_to_top/static/src/js/top.js"></script>
        	 </xpath>
        </template>
    
    </data>
</odoo>